{
  "name": "MyHomeworkSpace Button",
  "description": "A button that links directly to MyHomeworkSpace",
  "version": "1.2",
  "version_name": "Thanks for using MyHomeworkSpace Button!",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
      "128": "128px.png"
    },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Go To MyHomeworkSpace!",
      "default_icon": "16px.png"
  },
  "manifest_version": 2
}
